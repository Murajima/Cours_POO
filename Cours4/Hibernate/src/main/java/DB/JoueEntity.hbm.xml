<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB.JoueEntity" table="joue" schema="Films">
        <composite-id mapped="true" class="DB.JoueEntityPK">
            <key-property name="codeF" column="codeF"/>
            <key-property name="codeA" column="codeA"/>
        </composite-id>
        <property name="casting" column="casting"/>
    </class>
</hibernate-mapping>